<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" type="text/css" href="/css/app.css ">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
<style>

</style>
</head>
<body>
  <%- include('../partials/header')  %>
     
      <%- include('../partials/admin-side-navbar') %>
      <!-- Page content -->
      <div class="admin_content">
        <div class="slide">Welcome <%= user.username %></div>
        <div class="profile_content">
          <div class="profile_img"><img src="<%= user.profilePictureUrl %>" alt="Profile img"></div>
          <div class="profile_details">
            <ul style="list-style-type: none;">
              <li>Username : @<%= user.username %></li>
              <li>Author : <%= user.fullname.firstname + " " + user.fullname.lastname %> </li>
              <li>Mobile no. : <%= user.mobile_no %></li>
              <li>Email id : <%= user.email %></li>
              <li>Last Upadated on : <%= user.created_on %></li>
            </ul>
          </div>
        </div>
      </div>

      <script>
        const profile_button = document.querySelector("#profile_link");
        profile_button.classList.add("active");
      </script>
</body>
</html>
